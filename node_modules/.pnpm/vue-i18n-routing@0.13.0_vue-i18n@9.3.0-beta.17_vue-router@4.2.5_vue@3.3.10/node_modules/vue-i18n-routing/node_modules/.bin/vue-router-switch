#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lucas/Desktop/cubos-movies/node_modules/.pnpm/@intlify+vue-router-bridge@0.8.0_vue-router@4.2.5_vue@3.3.10/node_modules/@intlify/vue-router-bridge/bin/node_modules:/Users/lucas/Desktop/cubos-movies/node_modules/.pnpm/@intlify+vue-router-bridge@0.8.0_vue-router@4.2.5_vue@3.3.10/node_modules/@intlify/vue-router-bridge/node_modules:/Users/lucas/Desktop/cubos-movies/node_modules/.pnpm/@intlify+vue-router-bridge@0.8.0_vue-router@4.2.5_vue@3.3.10/node_modules/@intlify/node_modules:/Users/lucas/Desktop/cubos-movies/node_modules/.pnpm/@intlify+vue-router-bridge@0.8.0_vue-router@4.2.5_vue@3.3.10/node_modules:/Users/lucas/Desktop/cubos-movies/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lucas/Desktop/cubos-movies/node_modules/.pnpm/@intlify+vue-router-bridge@0.8.0_vue-router@4.2.5_vue@3.3.10/node_modules/@intlify/vue-router-bridge/bin/node_modules:/Users/lucas/Desktop/cubos-movies/node_modules/.pnpm/@intlify+vue-router-bridge@0.8.0_vue-router@4.2.5_vue@3.3.10/node_modules/@intlify/vue-router-bridge/node_modules:/Users/lucas/Desktop/cubos-movies/node_modules/.pnpm/@intlify+vue-router-bridge@0.8.0_vue-router@4.2.5_vue@3.3.10/node_modules/@intlify/node_modules:/Users/lucas/Desktop/cubos-movies/node_modules/.pnpm/@intlify+vue-router-bridge@0.8.0_vue-router@4.2.5_vue@3.3.10/node_modules:/Users/lucas/Desktop/cubos-movies/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@intlify+vue-router-bridge@0.8.0_vue-router@4.2.5_vue@3.3.10/node_modules/@intlify/vue-router-bridge/bin/switch.js" "$@"
else
  exec node  "$basedir/../../../../../@intlify+vue-router-bridge@0.8.0_vue-router@4.2.5_vue@3.3.10/node_modules/@intlify/vue-router-bridge/bin/switch.js" "$@"
fi
